$(function () {
  $('#supportDiv').hide();
  $('#aboutDiv').show();
  $('#about').click(function(){
	  $('#supportDiv').hide();
	  $('#aboutDiv').show();
	  $('#about').parent("li").addClass('active');
	  $('#support').parent("li").removeClass('active');
  });
  $('#support').click(function(){
	  $('#supportDiv').show();
	  $('#aboutDiv').hide();
	  $('#about').parent("li").removeClass('active');
	  $('#support').parent("li").addClass('active');
  });
});
</script>
